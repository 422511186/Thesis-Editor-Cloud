<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.cmgzs.mapper.FilesUploadMapper">
    <resultMap id="FilesUploadMap" type="com.cmgzs.domain.FilesUpload">
        <id column="file_key" property="fileKey"></id>
        <result column="file_name" property="fileName"></result>
        <result column="file_path" property="filePath"></result>
        <result column="status" property="status"></result>
        <result column="create_time" property="createTime"></result>
        <result column="create_by" property="createBy"></result>
    </resultMap>

    <insert id="insert">
        insert into files_upload
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="fileKey != null">file_key,</if>
            <if test="fileName != null">file_name,</if>
            <if test="filePath != null">file_path,</if>
            <if test="status != null">status,</if>
            <if test="createTime != null">create_time,</if>
            <if test="createBy != null">create_by,</if>
        </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="fileKey != null">#{fileKey},</if>
            <if test="fileName != null">#{fileName},</if>
            <if test="filePath != null">#{filePath},</if>
            <if test="status != null">#{status},</if>
            <if test="createTime != null">#{createTime},</if>
            <if test="createBy != null">#{createBy},</if>
        </trim>
    </insert>

    <delete id="delete">
        delete
        from files_upload
        where file_key = #{fileId}
    </delete>

    <select id="selectFiles" resultType="com.cmgzs.domain.FilesUpload">
        select *
        from files_upload
    </select>
</mapper>